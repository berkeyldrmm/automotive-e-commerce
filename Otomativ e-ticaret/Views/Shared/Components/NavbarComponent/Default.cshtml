@model List<Marka>

@foreach(var marka in Model){
    if (marka.UrunCesitId == 1)
    {
        <li class="dropdown">
            <a class="dropdown-toggle" href="" data-bs-toggle="dropdown"> @marka.MarkaAdi </a>
            <ul class="dropdown-menu">
                @foreach(var kategori in ViewBag.kategoriler)
                {
                    if (kategori.UrunCesitId == 1)
                    {
                        <li><a class="dropdown-item" href="https://localhost:7121/urunler/yag/@kategori.KategoriKod/?marka=@marka.MarkaKod">@kategori.KategoriAdi</a></li>
                    }
                }
            </ul>
        </li>
    }
}